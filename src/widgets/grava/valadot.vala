/* valac --pkg libgvc valadot.vala */

using Gvc;

int main (string[] args)
{
	Gvc.Context ctx = new Gvc.Context ();

	Graph graph = new Gvc.Graph ("my graph name", Gvc.GraphKind.AGDIGRAPHSTRICT);
	Gvc.Node node1 = graph.create_node ("node1");
	Gvc.Node node2 = graph.create_node ("node2");
	Gvc.Node node3 = graph.create_node ("node3");
	graph.create_edge (node1, node2);
	graph.create_edge (node1, node3);

	node1.safe_set ("label", new HtmlString ("<FONT POINT-SIZE=\"72\" FACE=\"Arial\">LARGE ARIAL TEXT</FONT>"), null);

	Gvc.Node? node = graph.get_first_node ();
	while (node != null)
	{
		node.safe_set ("color", "#000040", null);
		//node.safe_set ("shape", "note", null);
		node.safe_set ("shape", "box", null);
		node.safe_set ("fontcolor", "#EEEEFF", null);
		node.safe_set ("style", "filled", null);
		node.safe_set ("fillcolor", "#CCCCDD", null);
		node = graph.get_next_node (node);
	}

	graph.safe_set ("bgcolor", "#EEEEFF", null);
	graph.safe_set ("label", "This is my label", null);
	graph.safe_set ("fontsize", "21.0", null);
	graph.safe_set ("fontcolor", "#8080FF", null);

	ctx.layout (graph, "dot");

	// Instead of .render_filename (...), it's also possible
	// to call .render (graph, "dot", null) causing graphviz
	// to attach node coordinates in "pos" attributes without
	// actually drawing anything anywhere.
	ctx.render_filename (graph, "png", "output.png");
	ctx.render_filename (graph, "dot", "output.dot");
	ctx.render_filename (graph, "xdot", "output.xdot");

